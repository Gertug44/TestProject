<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.HouseEntity" table="house" schema="public" catalog="pflb_trainingcenter">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="floor_count">
            <column name="floor_count" sql-type="integer"/>
        </property>
        <property name="price">
            <column name="price" sql-type="numeric(19,2)"/>
        </property>
    </class>
</hibernate-mapping>